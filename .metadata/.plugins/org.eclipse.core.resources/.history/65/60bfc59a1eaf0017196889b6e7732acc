package javabasics;

public class numberpattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int a=44;
		for(int i=6;i>=1; i--)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print((char)a);
			}
			System.out.println(" ");
			a++;
		
		}
		
		
		
		
	}

}
